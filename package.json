{
  "name": "enjinn",
  "version": "1.1.0",
  "description": "A library management tool for Denon Engine PRIME",
  "author": "SHAYDED <shayded@shayded.com> (http://shayded.com)",
  "homepage": "https://github.com/rshea0/enjinn#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rshea0/enjinn.git"
  },
  "bugs": {
    "url": "https://github.com/rshea0/enjinn/issues",
    "email": "shayded@shayded.com"
  },
  "keywords": [
    "enjinn",
    "engine",
    "genie",
    "engine prime",
    "denon",
    "dj",
    "denon dj",
    "relocate library",
    "missing tracks",
    "smart playlist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "enjinn": "./bin/run"
  },
  "main": "lib/index.js",
  "oclif": {
    "bin": "enjinn",
    "commands": "./lib/commands",
    "helpClass": "./lib/help",
    "hooks": {
      "prerun": [
        "./lib/hooks/prerun/prerun-engine-library"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 2,
      "message": "Update available from <%=chalk.yellow(config.version)%> to <%=chalk.greenBright(latest)%>"
    }
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "scripts": {
    "publish": "np --no-tests --message v%s",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "@oclif/plugin-update": "1.3.10",
    "@oclif/plugin-warn-if-update-available": "1.7.0",
    "ajv": "8.1.0",
    "chalk": "2.4.2",
    "chalk-animation": "1.6.0",
    "cli-ux": "5.5.1",
    "conf": "10.0.1",
    "figlet": "1.5.0",
    "gradient-string": "1.2.0",
    "inquirer": "8.0.0",
    "js-yaml": "4.1.0",
    "knex": "0.95.4",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "sqlite3": "5.0.2",
    "terminal-link": "2.1.1",
    "true-case-path": "2.2.1",
    "type-fest": "0.21.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@types/chalk": "2.2.0",
    "@types/chalk-animation": "1.6.0",
    "@types/figlet": "1.5.1",
    "@types/gradient-string": "1.1.1",
    "@types/inquirer": "7.3.1",
    "@types/js-yaml": "4.0.1",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.41",
    "rimraf": "2.7.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
